{
  "name": "account-service-server",
  "version": "0.0.0",
  "description": "Account service server side example",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib/",
    "compile": "babel src/ --out-dir lib/ --copy-files --source-maps",
    "build": "npm-run-all clean compile",
    "gen-private-key": "openssl genpkey -algorithm RSA -out keys/private.pem -pkeyopt rsa_keygen_bits:2048",
    "export-publickey": "openssl rsa -pubout -in keys/private.pem -out keys/public.pem",
    "genkeypair": "npm-run-all gen-private-key export-publickey",
    "debug": "nodemon src/index.js --ignore *.test.js --inspect --exec babel-node",
    "start": "node lib/index.js"
  },
  "author": "Vadim Sartakov",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "backend-tools": "file:../../../",
    "bcryptjs": "^2.4.3",
    "client-oauth2": "^4.2.1",
    "connect-mongo": "^2.0.1",
    "express-oauth-server": "^2.0.0",
    "express-session": "^1.15.6",
    "handlebars": "^4.0.12",
    "jsonwebtoken": "^8.3.0",
    "node-sspi": "^0.2.3",
    "nodemailer": "^4.6.8",
    "uuid": "^3.3.2",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
