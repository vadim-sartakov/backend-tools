{
  "name": "account-service-server",
  "version": "0.0.0",
  "description": "Account service server side example",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf lib/",
    "compile": "babel src/ --out-dir lib/ --copy-files --source-maps",
    "build": "npm-run-all clean compile",
    "debug": "cross-env NODE_ENV=development nodemon src/index.js --ignore *.test.js --inspect --exec babel-node",
    "start": "cross-env NODE_ENV=production node lib/index.js"
  },
  "author": "Vadim Sartakov",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
