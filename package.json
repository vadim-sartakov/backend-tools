{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "lib/server.js",
  "scripts": {
    "unit-test": "mocha **/*unit.test.js --require babel-core/register --require babel-polyfill",
    "int-test": "cross-env NODE_ENV=test mocha **/*int.test.js --require babel-core/register --require babel-polyfill --timeout 10000",
    "eslint": "eslint --ignore-path .gitignore ./",
    "clean": "rimraf lib/",
    "compile": "babel src/ --ignore test.js,/tests --out-dir lib/ --copy-files --source-maps",
    "build": "npm-run-all clean compile",
    "debug": "cross-env NODE_ENV=development nodemon src/server.js --ignore *.test.js --inspect --exec babel-node",
    "start": "cross-env NODE_ENV=production node lib/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "debug": "^3.2.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "http-link-header": "^0.8.0",
    "i18next": "^11.9.0",
    "i18next-express-middleware": "^1.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.2.17",
    "mongoose-autopopulate": "^0.8.1",
    "mongoose-unique-validator": "^2.0.2",
    "qs": "^6.5.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^5.6.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.3.0"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
