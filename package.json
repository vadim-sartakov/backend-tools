{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "lib/server.js",
  "scripts": {
    "unit-test": "jest --watch \".+(\\.test)\\.js\"",
    "it-test": "cross-env NODE_ENV=test jest \".+(\\.itest)\\.js\" --detectOpenHandles",
    "eslint": "eslint --ignore-path .gitignore ./",
    "clean": "rimraf lib/",
    "build": "babel src/ --ignore test.js,itest.js --out-dir lib/ --copy-files --source-maps",
    "rebuild": "npm-run-all clean build",
    "debug": "cross-env NODE_ENV=development nodemon src/server.js --ignore *.test.js --ignore *.itest.js --inspect --exec babel-node",
    "start": "cross-env NODE_ENV=production node lib/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.9",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^5.4.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": ["transform-object-rest-spread"]
  }
}
